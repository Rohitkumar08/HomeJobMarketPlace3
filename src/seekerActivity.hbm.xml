<?xml version='1.0' encoding='UTF-8'?>  

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
 <hibernate-mapping>
	<class name = "com.hmj.model.SeekerActivity" table="seeker_activity">
	<id name="id">
	<generator class="increment"/></id>
	<!-- <discriminator column="user_type"/> -->
	
	<property name="message" column="action_performed" not-null="true"/>
	<property name="uid" column="seeker_id" not-null="true"/>
	
	</class>
	
	
	
	
	
</hibernate-mapping>	